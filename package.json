{
  "name": "prompt-quest",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install-all": "npm install && npm install --prefix Frontend",
    "dev": "npm run dev --prefix Frontend",
    "dev:frontend": "npm run dev --prefix Frontend",
    "dev:backend": "python Backend/app.py",
    "dev:all": "concurrently \"npm run dev --prefix Frontend\" \"python Backend/app.py\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
